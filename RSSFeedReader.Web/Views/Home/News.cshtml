@model IEnumerable<RSSFeedReader.Logic.Common.Models.NewsItemDto>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>News</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Url</th>
            <th>Description</th>
            <th>Publication Date</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@item.Url</td>
            <td>@item.Description</td>
            <td>@item.PublicationDate</td>
        </tr>
        }
    </table>
    <p>
        
        @Html.ActionLink("Back to List", "Index")
    </p>
</body>
</html>
